{
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": ["error", { "trailingComma": "all", "singleQuote": true }],
    "no-underscore-dangle": "off",
    "no-use-before-define": ["error", { "functions": false }],
    "global-require": "off",
    "import/no-dynamic-require": "off"
  }
}
